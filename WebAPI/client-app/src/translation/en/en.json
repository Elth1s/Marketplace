            "about": {
                "aboutUs": {
                    "title": "About us",
                    "desc": "Mall is the largest marketplace in Ukraine, where goods from tens of thousands of entrepreneurs from all over the country are sold. You can find literally everything here. We provide a guarantee because we believe that online shopping should be as safe as possible."
                },
                "ourGoal": {
                    "title": "Our goal",
                    "desc": "We do everything to make it pleasant and easy for customers to find what they want. After all, we have everything! We always fulfill the wishes of our customers and try to save them from unpleasant situations."
                },
                "convenientDelivery": {
                    "title": "Convenient delivery",
                    "desc": "Of course, any product can be ordered with delivery. We deliver orders within one day in Kyiv, in Ukraine - the next day. All - without prepayment, if necessary - on credit. Payment in cash or non-cash, as you prefer."
                },
                "ourTeam": {
                    "title": "Our team is our everything!"
                }
